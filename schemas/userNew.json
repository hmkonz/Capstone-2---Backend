          "examples": [
              "hmkonz@yahoo.com"
          ]
      },
      "firstNameBilling": {
          "type": "string",
          "default": "",
          "title": "The firstNameBilling Schema",
          "examples": [
              "Winston"
          ]
      },
      "lastNameBilling": {
          "type": "string",
          "default": "",
          "title": "The lastNameBilling Schema",
          "examples": [
              "Konz"
          ]
      },
      "billingAddress": {
          "type": "string",
          "default": "",
          "title": "The billingAddress Schema",
          "examples": [
              "400 N. Park Ave. Unit 12B Breckenridge Colorado 80424"
          ]
      },
        "firstNameShipping": {
            "type": "string",
            "default": "",
            "title": "The firstNamShippinge Schema",
            "examples": [
                "Winston"
            ]
        },
        "lastNameShipping": {
            "type": "string",
            "default": "",
            "title": "The lastNameShipping Schema",
            "examples": [
                "Konz"
            ]
        },
        "shippingAddress": {
          "type": "string",
          "default": "",
          "title": "The shippingAddress Schema",
          "examples": [
              "400 N. Park Ave. Unit 12B Breckenridge Colorado 80424"
          ]
      },
      "phone": {
          "type": "string",
          "default": "",
          "title": "The phone Schema",
          "examples": [
              "617-794-9575"
          ]
      },
      "password": {
          "type": "string",
          "default": "",
          "title": "The password Schema",
          "examples": [
              "qwerty"
          ]
      },
      "stripeCustomerId": {
           "type": "string",
           "default": "",
           "title": "The stripeCustomerId Schema",
           "examples": [
            "asdf1234"
           ]

      }
  },
  "examples": [{
      "email": "hmkonz@yahoo.com",
      "firstNameBilling": "Winston",
      "lastNameBilling": "Konz",
      "billingAddress": "400 N. Park Ave. Unit 12B Breckenridge Colorado 80424",
      "firstNameShipping": "Winston",
      "lastNameShipping": "Konz",
      "shippingAddress": "400 N. Park Ave. Unit 12B Breckenridge Colorado 80424",
      "phone": "617-794-9575",
      "password": "qwerty",
      "stripeCustomerId": "asdf1234"
  }]
}
