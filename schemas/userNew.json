{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
      "email",
      "password"
  ],
  "properties": {
      "email": {
          "type": "string",
          "default": "",
          "title": "The email Schema",
          "examples": [
              "hmkonz@yahoo.com"
          ]
      },
      "name": {
          "type": "string",
          "default": "",
          "title": "The name Schema",
          "examples": [
              "Winston"
          ]
      },
      "password": {
          "type": "string",
          "default": "",
          "title": "The password Schema",
          "examples": [
              "qwerty"
          ]
      },
      "stripe_customer_id": {
           "type": "string",
           "default": "",
           "title": "The stripeCustomerId Schema",
           "examples": [
            "asdf1234"
           ]

      }
  },
  "examples": [{
      "email": "hmkonz@yahoo.com",
      "name": "Winston",
      "password": "qwerty",
      "stripe_customer_id": "asdf1234"
  }]
}
