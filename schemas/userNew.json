{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
      "email",
      "password"
  ],
  "properties": {
      "email": {
          "type": "string",
          "default": "",
          "title": "The email Schema",
          "examples": [
              "hmkonz@yahoo.com"
          ]
      },
      "first_name_billing": {
          "type": "string",
          "default": "",
          "title": "The firstNameBilling Schema",
          "examples": [
              "Winston"
          ]
      },
      "last_name_billing": {
          "type": "string",
          "default": "",
          "title": "The lastNameBilling Schema",
          "examples": [
              "Konz"
          ]
      },
      "billing_address": {
          "type": "string",
          "default": "",
          "title": "The billingAddress Schema",
          "examples": [
              "400 N. Park Ave. Unit 12B Breckenridge Colorado 80424"
          ]
      },
        "first_name_shipping": {
            "type": "string",
            "default": "",
            "title": "The firstNamShippinge Schema",
            "examples": [
                "Winston"
            ]
        },
        "last_name_shipping": {
            "type": "string",
            "default": "",
            "title": "The lastNameShipping Schema",
            "examples": [
                "Konz"
            ]
        },
        "shipping_address": {
          "type": "string",
          "default": "",
          "title": "The shippingAddress Schema",
          "examples": [
              "400 N. Park Ave. Unit 12B Breckenridge Colorado 80424"
          ]
      },
      "phone": {
          "type": "string",
          "default": "",
          "title": "The phone Schema",
          "examples": [
              "617-794-9575"
          ]
      },
      "password": {
          "type": "string",
          "default": "",
          "title": "The password Schema",
          "examples": [
              "qwerty"
          ]
      },
      "stripe_customer_id": {
           "type": "string",
           "default": "",
           "title": "The stripeCustomerId Schema",
           "examples": [
            "asdf1234"
           ]

      }
  },
  "examples": [{
      "email": "hmkonz@yahoo.com",
      "first_name_billing": "Winston",
      "last_name_billing": "Konz",
      "billing_address": "400 N. Park Ave. Unit 12B Breckenridge Colorado 80424",
      "first_name_shipping": "Winston",
      "last_name_shipping": "Konz",
      "shipping_address": "400 N. Park Ave. Unit 12B Breckenridge Colorado 80424",
      "phone": "617-794-9575",
      "password": "qwerty",
      "stripe_customer_id": "asdf1234"
  }]
}
