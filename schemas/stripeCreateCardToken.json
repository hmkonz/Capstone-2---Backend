{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "card"
    ],
    "properties": {
        "card": {
            "type": "object",
            "default": {},
            "title": "The card Schema",
            "required": [
                "number",
                "exp_month",
                "exp_year",
                "cvc",
                "name",
                "addressLine1",
                "addressLine2",
                "addressCity",
                "addressState",
                "addressZip",
                "addressCountry"
            ],
            "properties": {
                "number": {
                    "type": "string",
                    "default": "",
                    "title": "The number Schema",
                    "examples": [
                        "4242424242424242"
                    ]
                },
                "exp_month": {
                    "type": "string",
                    "default": "",
                    "title": "The exp_month Schema",
                    "examples": [
                        "6"
                    ]
                },
                "exp_year": {
                    "type": "string",
                    "default": "",
                    "title": "The exp_year Schema",
                    "examples": [
                        "2024"
                    ]
                },
                "cvc": {
                    "type": "string",
                    "default": "",
                    "title": "The cvc Schema",
                    "examples": [
                        "314"
                    ]
                },
                "name": {
                    "type": "string",
                    "default": "",
                    "title": "The name Schema",
                    "examples": [
                        "Heather Konz"
                    ]
                },
                "addressLine1": {
                    "type": "string",
                    "default": "",
                    "title": "The address_line1 Schema",
                    "examples": [
                        "15 Cooney Court"
                    ]
                },
                "addressLine2": {
                    "type": "null",
                    "default": null,
                    "title": "The address_line2 Schema",
                    "examples": [
                        null
                    ]
                },
                "addressCity": {
                    "type": "string",
                    "default": "",
                    "title": "The address_city Schema",
                    "examples": [
                        "Blue River"
                    ]
                },
                "addressState": {
                    "type": "string",
                    "default": "",
                    "title": "The address_state Schema",
                    "examples": [
                        "CO"
                    ]
                },
                "addressZip": {
                    "type": "string",
                    "default": "",
                    "title": "The address_zip Schema",
                    "examples": [
                        "80424"
                    ]
                },
                "addressCountry": {
                    "type": "string",
                    "default": "",
                    "title": "The address_country Schema",
                    "examples": [
                        "US"
                    ]
                }
            },
            "examples": [{
                "number": "4242424242424242",
                "exp_month": "6",
                "exp_year": "2024",
                "cvc": "314",
                "name": "Heather Konz",
                "addressLine1": "15 Cooney Court",
                "addressLine2": null,
                "addressCity": "Blue River",
                "addressState": "CO",
                "addressZip": "80424",
                "addressCountry": "US"
            }]
        }
    },
    "examples": [{
