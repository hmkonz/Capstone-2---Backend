{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "product_name",
        "product_price",
        "user_id",
        "product_id"
    ],
    "properties": {
        "product_name": {
            "type": "string",
            "default": "",
            "title": "The productName Schema",
            "examples": [
                "Bison"
            ]
        },
        "product_price": {
            "type": "string",
            "default": "",
            "title": "The productPrice Schema",
            "examples": [
                "98.49"
            ]
        },
        "user_id": {
            "type": "number",
            "default": "",
            "title": "The userId Schema",
            "examples": [
                "3"
            ]
        },
        "product_id": {
            "type": "string",
            "default": "",
            "title": "The productId Schema",
            "examples": [
                "4"
            ]
        }
    },
    "examples": [{
        "product_name": "Bison",
        "product_price": "98.49",
        "user_id": "3",
        "product_id": "4"
    }]
}