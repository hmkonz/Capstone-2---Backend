      "password": {
          "type": "string",
          "default": "",
          "title": "The password Schema",
          "examples": [
              "qwerty1234"
          ]
      }
  },
  "examples": [{
      "email": "winston@gmail.com",
      "password": "qwerty1234"
  }]
}

